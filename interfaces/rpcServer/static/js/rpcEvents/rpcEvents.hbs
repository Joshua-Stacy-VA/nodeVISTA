<div class="events">
    <div class="rpc-events submenu">

        <span class="glyphicon glyphicon-filter" alt="Emulated RPC filter label."></span>
        <div id="filter" class="rpc-event-select"></div>

        <a href="#" class="clear-events-list" title="Clear RPC Events List">
            <span class="rpcEvents glyphicon glyphicon-ban-circle" aria-hidden="true"></span>
        </a>
        <span class="mvdm-events-status">
            <span>
                <strong class="submenu-label">Total:</strong>
                <span class="event-count-total"></span>
            </span>
            <span>
                <strong class="submenu-label">Local RPC Runner:</strong>
                <span class="event-count-local-rpc-runner"></span>
            </span>
            <span>
                <strong class="submenu-label">Emulated:</strong>
                <span class="event-count-emulated"></span>
            </span>
            <span>
                <strong class="submenu-label">Hardcode:</strong>
                <span class="event-count-hardcode"></span>
            </span>
        </span>
    </div>

    <div id="events-table"></div>

    <div id="event-modal-container"></div>
</div>
