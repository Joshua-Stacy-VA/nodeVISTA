<table class="table table-condensed table-striped table-bordered table-hover">
    <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Domain</th>
        <th>Type</th>
        <th>User</th>
        <th>Facility</th>
    </tr>
    </thead>
    <tbody>
    {{#each events as |event|}}
        <tr class="event-row" data-cid="{{event.cid}}"> <!-- cid is internal backbone model client id -->
            <td>{{event.date}}</td>
            <td>{{event.time}}</td>
            <td>{{event.domain}}</td>
            <td class="text-uppercase">{{event.type}}</td>
            <td>{{event.user.name}} ({{event.user.id}})</td>
            <td>{{event.facility.name}} ({{event.facility.id}} / {{event.facility.stationNumber}})</td>
        </tr>
    {{/each}}
    </tbody>
</table>