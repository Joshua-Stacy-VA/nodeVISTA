<table class="table table-condensed table-striped table-bordered table-hover">
    <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Domain</th>
        <th>Type</th>
        <th>User ID</th>
        <th>User Name</th>
        <th>Facility ID</th>
        <th>Facility Name</th>
        <th>Station Number</th>
    </tr>
    </thead>
    <tbody>
    {{#each events as |event|}}
        <tr class="event-row" data-cid="{{event.cid}}"> <!-- cid is internal backbone model client id -->
            <td>{{event.date}}</td>
            <td>{{event.time}}</td>
            <td>{{event.domain}}</td>
            <td>{{toUpperCase event.type}}</td>
            <td>{{event.user.id}}</td>
            <td>{{event.user.name}}</td>
            <td>{{event.facility.id}}</td>
            <td>{{event.facility.name}}</td>
            <td>{{event.facility.stationNumber}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>